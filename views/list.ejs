<%- include("header") -%>
    <div class="box" id="heading">
      <h1><%= listTitle %></h1>
    </div>

    <div class="box">
      <% for (let i = 0; i < newListItems.length; i++) { %>
      <div class="item">
        <input type="checkbox" name="" id="" autocomplete="off" />
        <p><%= newListItems[i] %></p>
      </div>
      <% } %>

      <div class="item">
        <form action="/views/list" method="POST">
          <input
            type="text"
            id="newItem"
            name="newItem"
            placeholder="To do item"
          />
          <button type="submit" name="list" value=<%= listTitle %>>+</button>
        </form>
      </div>
    </div>
  <%- include("footer") -%>
